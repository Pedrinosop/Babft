local url = "https://raw.githubusercontent.com/Pedrinosop/Babft/refs/heads/main/autoBumain"
local success, content = pcall(game.HttpGet, game, url)

if success then
    writefile("SOPERASCRIPT.lua", content)
    task.wait(0.5)
    loadstring(readfile("SOPERASCRIPT.lua"))()
    print("LOAD")
else
    warn("ERROR copy script in url")
end
